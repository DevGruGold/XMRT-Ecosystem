{
  "version": 2,
  "buildCommand": "npm install -g pnpm@10.4.1 && cd frontend/xmrt-dao-frontend && pnpm install && pnpm run build",
  "outputDirectory": "frontend/xmrt-dao-frontend/dist",
  "installCommand": "npm install -g pnpm@10.4.1 && pnpm install --store=node_modules/.pnpm-store",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}

