{
  "version": 2,
  "name": "xmrt-ecosystem",
  "routes": [
    {
      "src": "^/$",
      "dest": "/index.html"
    },
    {
      "src": "/health",
      "dest": "/api/index.py"
    },
    {
      "src": "/api/agents/([^/]+)",
      "dest": "/api/agents.py"
    },
    {
      "src": "/api/agents",
      "dest": "/api/agents.py"
    },
    {
      "src": "/api/tick",
      "dest": "/api/tick.py"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ]
}
