
{
  "version": 2,
  "builds": [
    {
      "src": "./package.json",
      "use": "@vercel/node@2.10.3",
      "config": {
        "distDir": "dist",
        "includeFiles": ["backend/**", "python_service/**", "requirements.txt"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.html"
    }
  ]
}