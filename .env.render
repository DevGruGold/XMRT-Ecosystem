# XMRT-Ecosystem Render Environment Variables Template
# Copy these to your Render dashboard Environment Variables section

# Flask Configuration
FLASK_ENV=production
FLASK_APP=app.py
SECRET_KEY=your-secure-secret-key-here-change-this

# Python Configuration
PYTHONUNBUFFERED=1
WEB_CONCURRENCY=1

# Application Configuration
DEBUG=False
TESTING=False

# Database Configuration (if needed)
# DATABASE_URL=your-database-url-here
# REDIS_URL=your-redis-url-here

# API Keys (add as needed)
# OPENAI_API_KEY=your-openai-key-here
# ANTHROPIC_API_KEY=your-anthropic-key-here

# GitHub Integration (if needed)
# GITHUB_TOKEN=your-github-token-here
# GITHUB_WEBHOOK_SECRET=your-webhook-secret-here

# Monitoring (optional)
# SENTRY_DSN=your-sentry-dsn-here

# Custom Application Settings
APP_NAME=XMRT-Ecosystem
VERSION=2.0
ENVIRONMENT=production
