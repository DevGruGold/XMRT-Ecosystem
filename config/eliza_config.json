{
  "services": {
    "redis": {
      "host": "localhost",
      "port": 6379,
      "enabled": false
    },
    "memory_api": {
      "port": 5001,
      "enabled": false
    },
    "eliza_api": {
      "port": 5000,
      "enabled": true
    }
  },
  "eliza": {
    "confidence_thresholds": {
      "autonomous": 0.85,
      "advisory": 0.60,
      "emergency": 0.95
    },
    "memory": {
      "semantic_search": false,
      "embedding_model": "all-MiniLM-L6-v2"
    },
    "monitoring": {
      "health_check_interval": 30,
      "alert_threshold": 0.7
    }
  },
  "logging": {
    "level": "INFO",
    "file": "logs/eliza_orchestrator.log"
  }
}